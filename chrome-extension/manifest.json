{
    "manifest_version": 2,
    "name": "Kite Extension",
    "version": "1.0.0",
    "permissions": [
      "storage"
    ],
    "content_scripts": [
        { 
          "matches": [
            "https://kite.zerodha.com/holdings",
            "https://kite3.zerodha.com/holdings"
          ],
          "css": ["utils/popup.css"],
          "js": ["tp/jquery.min.js", "utils/utils.js", "kite-content.js"]
        },
        {
          "matches": [
            "https://angelurl/*"
          ],
          "js": ["utils/utils.js", "angel-content.js"]
        }
      ]
  }